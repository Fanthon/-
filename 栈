#include <stack>
#include <iostream>
using namespace std;

int main()
{
	stack<int> s;  //声明存储int类型数据的栈
	s.push(1);	   //{}     -> {1}
	s.push(2);	   //{1}    -> {1,2}
	s.push(3);	   //{1.2}  -> {1,2,3}
	cout<<s.top()<<endl;
	s.pop();	   //移除栈顶的3
	cout<<s.top()<<endl;
	s.pop();	   //移除栈顶的2
	cout<<s.top()<<endl;
	s.pop();
	return 0; 
 } 
